{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },    
    "ListenAddress": "https://localhost",
    "ListenPort": "7264",
    "DatabaseSchema": "dbo",
    "TestDataPrefix": "t_testdata_%",
    "ConnectionStrings": {
        "JAAADatabase": "DATABASECONNECTIONSTRING",
        "AzureWebJobsStorage": "STORAGECONNECTIONSTRING"
    },
    "LocalRunner": {
        "CreateNoWindow": false
    },
    "ImagesContainer": "jaaa",
    "Endpoints": {
        "Accounts": "api/Accounts",
        "AccountsRegister": "api/Accounts/register",
        "Images": "api/Images",
        "ImagesGetImage": "api/Images/{0}/{1}/{2}/{3}",
        "ImagesGetImageWithVersion": "api/Images/{0}/{1}/{2}/{3}/{4}",
        "ImagesDeleteWholeFile": "api/Images/{0}",
        "ImagesDeleteWholeFileWithMiniatures": "api/Images/{0}/{1}",
        "ImagesDeleteWholeImageWithMiniatureSize": "api/Images/{0}/{1}/{2}",
        "Groups": "api/Groups",
        "GroupsDeleteById": "api/Groups/{0}",
        "Shares": "api/Shares",
        "SharesAddUserShare": "api/Shares/Users",
        "SharesGetUserSharesByUserId": "api/Shares/Users/{0}",
        "SharesDeleteUserShareByFileAndUserId": "api/Shares/Users/{0}/{1}",
        "SharesAddGroupShare": "api/Shares/Groups",
        "SharesGetGroupShareByGroupId": "api/Shares/Groups/{0}",
        "SharesDeleteGroupShareByFileAndGroupId": "api/Shares/Groups/{0}/{1}"
    }
}